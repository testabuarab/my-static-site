{% block js %}
    {% minify js %}

        <script>


            {%if entry.hasSteps or entry.alsmahBaltaalykat or entry.hasItems or entry.hasFaqs%}

            {%if entry.hasSteps %}
            current = 1;
            stepsNode = document.querySelector("#steps");
            contentsNode = document.querySelector("#contents");

            totalSteps = stepsNode.children.length;

            stepsNode.querySelectorAll("button.stepsbtn").forEach((step) => {
                step.addEventListener("click", (e) => {
                    const targetStep = parseInt(e.target.dataset.step);

                    contentsNode
                        .querySelectorAll(".scontent")
                        .forEach((content) => content.classList.remove("active"));
                    stepsNode
                        .querySelectorAll("button.stepsbtn")
                        .forEach((content) => content.classList.remove("active"));

                    contentsNode
                        .querySelector(`.scontent[data-step="${targetStep}"]`)
                        .classList.add("active");
                    stepsNode
                        .querySelector(`button.stepsbtn[data-step="${targetStep}"]`)
                        .classList.add("active");

                    stepsNode
                        .querySelectorAll("div")
                        .forEach((content) => content.classList.remove("active"));
                    if (targetStep - 1 > 0) {
                        const num = targetStep - 1;
                        for (let i = 1; i <= num; i++) {
                            stepsNode
                                .querySelector(`div:nth-of-type(${i})`)
                                .classList.add("active");
                        }
                    }
                });
            });
            {%endif%}
            {%if entry.hasFaqs %}
            const items = document.querySelectorAll('.accordion button');

            function toggleAccordion() {
                const itemToggle = this.getAttribute('aria-expanded');

                for (i = 0; i < items.length; i++) {
                    items[i].setAttribute('aria-expanded', 'false');
                }

                if (itemToggle == 'false') {
                    this.setAttribute('aria-expanded', 'true');
                }
            }

            items.forEach((item) => item.addEventListener('click', toggleAccordion));

            {%endif%}
            {%if entry.alsmahBaltaalykat%}
            function loadmores(){var elementcom=document.getElementsByClassName("hiddencomments");var morbtn=document.getElementById("loadmores");for(var i=0;i<elementcom.length;i++){elementcom[i].classList.remove("hidden");}
                morbtn.style.display="none";}
            {%endif%}
            {%endif%}
        </script>
