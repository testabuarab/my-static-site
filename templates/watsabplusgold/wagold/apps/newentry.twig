                            </th>
                            <td class="spec-value">
                                {{ appVersion }}
                            </td>
                        </tr>
                        <tr class="spec">
                            <th class="spec-title icon-calendar-check-o">
                                {{ "Release date"|t }}
                            </th>
                            <td class="spec-value">
                                {{ entry.akhrThdyth ? entry.akhrThdyth|date("Y-m-d", locale="en-GB"):entry.dateUpdated|date("Y-m-d", locale="en-GB") }}
